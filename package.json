{
	"name": "@wdns/vuetify-resize-drawer",
	"version": "3.3.1",
	"description": "The vuetify-resize-drawer component extends the functionality of the v-navigation-drawer so that it is resizable by the user.",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"main": "dist/vuetify-resize-drawer.cjs.js",
	"module": "dist/vuetify-resize-drawer.es.js",
	"types": "dist/plugin/types/index.d.ts",
	"scripts": {
		"dev": "NODE_OPTIONS='--no-warnings' vite",
		"watch": "pnpm dev",
		"play": "sh src/playground/configs/build.sh && NODE_ENV=playground pnpm dev",
		"build": "tsc -p tsconfig.build.json && npm run test:build && vite build --config vite.build.config.mts",
		"build:docs": "vite build",
		"predeploy": "npm run build",
		"deploy": "gh-pages -d docs",
		"prepublishOnly": "npm run build",
		"lint": "eslint src/**/*.{ts,vue} --max-warnings 20",
		"prepare": "husky install",
		"test:dev": "NODE_OPTIONS='--no-warnings' vitest",
		"test:all": "vitest --run",
		"test:build": "vitest --run --bail 1",
		"cy:run": "npx cypress run --headless --browser electron",
		"cy:run:dev": "cypress run --browser chrome",
		"cy:open": "cypress open --browser chrome"
	},
	"lint-staged": {
		"src/**/*.{js,ts,vue}": [
			"npm run lint"
		]
	},
	"author": "WebDevNerdStuff & Bunnies... lots and lots of bunnies! <webdevnerdstuff@gmail.com> (https://webdevnerdstuff.com)",
	"funding": [
		{
			"type": "paypal",
			"url": "https://paypal.me/webdevnerdstuff"
		},
		{
			"type": "patreon",
			"url": "https://www.patreon.com/WebDevNerdStuff"
		}
	],
	"license": "MIT",
	"files": [
		"dist/*",
		"LICENSE.md",
		"README.md"
	],
	"repository": "https://github.com/webdevnerdstuff/vuetify-resize-drawer",
	"bugs": {
		"url": "https://github.com/webdevnerdstuff/vuetify-resize-drawer/issues"
	},
	"homepage": "https://webdevnerdstuff.github.io/vuetify-resize-drawer/",
	"keywords": [
		"vuetify-resize-drawer",
		"vuetifyResizeDrawer",
		"v-resize-drawer",
		"vResizeDrawer",
		"vuetify",
		"vuetify3",
		"navigation drawer",
		"v-navigation-drawer",
		"VNavigationDrawer",
		"api",
		"drawer",
		"resize",
		"resizable",
		"vue",
		"vue2",
		"component",
		"javascript",
		"webdevnerdstuff",
		"wdns"
	],
	"dependencies": {
		"vue": "^3.5.17",
		"vuetify": "^3.8.11"
	},
	"devDependencies": {
		"@eslint/js": "^9.30.0",
